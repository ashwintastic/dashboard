  "babel": {
    "presets": [
      "react",
      "stage-0",
      "latest"
    ],
    "plugins": [
      "syntax-trailing-function-commas",
      "transform-async-to-generator",
      "transform-es2015-destructuring",
      "transform-es2015-parameters",
      "transform-es2015-duplicate-keys",
      "transform-es2015-modules-commonjs",
      "transform-exponentiation-operator",
      "transform-runtime"
    ],
    "env": {
      "test": {
        "plugins": [
          "rewire"
        ]
      }
    }
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "globals": {
      "__DEV__": true
    },
    "env": {
      "browser": true
    },
    "rules": {
      "generator-star-spacing": 0,
      "import/no-extraneous-dependencies": 0,
      "jsx-quotes": [
        2,
        "prefer-double"
      ],
      "no-confusing-arrow": 0,
      "react/jsx-filename-extension": 0,
      "react/jsx-quotes": 0,
      "react/require-extension": 0,
      "no-unused-vars": [
        "error",
        {
          "vars": "all",
          "args": "after-used"
        }
      ],
      "no-underscore-dangle": 0
    }
  },
  "stylelint": {
    "ignoreFiles": [
      "./src/public/**/*.css"
    ],
    "extends": "stylelint-config-standard",
    "rules": {
      "string-quotes": "single",
      "property-no-unknown": [
        true,
        {
          "ignoreProperties": [
            "composes"
          ]
        }
      ],
      "selector-pseudo-class-no-unknown": [
        true,
        {
          "ignorePseudoClasses": [
            "global",
            "local"
          ]
        }
      ]
    }
  },
  "scripts": {
    "eslint": "eslint src tools",
    "stylelint": "stylelint \"src/**/*.css\"",
    "lint": "yarn run eslint && yarn run stylelint",
    "test": "mocha \"src/**/*.test.js\" --require test/setup.js --compilers js:babel-register",
    "test:watch": "yarn run test -- --reporter min --watch",
    "clean": "babel-node tools/run clean",
    "copy": "babel-node tools/run copy",
    "bundle": "babel-node tools/run bundle",
    "build": "yarn run upgrade && yarn install --force && babel-node tools/run build",
    "build:prod": "yarn install --force && babel-node tools/run build --release",
    "deploy": "babel-node tools/run deployToAzureWebApps",
    "render": "babel-node tools/run render",
    "start": "yarn run upgrade && yarn install --force && babel-node tools/run start",
    "serve": "babel-node tools/run start",
    "upgrade": "yarn upgrade botworx-utils botworx-scheduler botworx-schema",
    "validate": "yarn ls"
  }
}
